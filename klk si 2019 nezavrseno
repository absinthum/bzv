#include <stdio.h>
#define MAX 1000
int main() {
	int n = 0, a[MAX], i, p[MAX];
	int k = n;
	printf("Duzina niza: ");
	do {
		scanf("%d", &n);
	} while (n < 0 && n > MAX);
	for (i = 0; i < n; i++) {
		p[i] = 1;
	}
	for (i = 0; i < n; i++) {
		scanf("%d", a + i);
		for (int k = 0; k < i; k++) {
			if (a[i] == a[k]) {
				p[i] = 0;
				k--;
				
			}
		}
	}
	for (i = 0; i < n; i++) {
		printf("%d ", a[i]);
	}
	printf("\n");
	int j = 0;
	/*for (i = 0; i < n; i++) {
		for (j = i; j < n; j++) {
			if (a[i] == a[j]) {
				p[i] = 0;
				k--;
			}
		}
	}*/
	j = 0;
	for (i = 0; i < n; i++) {
		if (p[i]) {
			printf("%d ", a[i]);
		}
	}
	for (i = 0; i < n - 1; i++) {
		for (j = i + 1; j < n; j++) {
			if (a[i] < a[j] && p[i]) {
				k = a[i];
				a[i] = a[j];
				a[j] = k;
			}
		}
	}
	printf("\n");
	for (i = 0; i < 3; i++) {
		if (k >= 3) printf("%d\n", a[i]);
		else printf("X\n");
	}
	system("pause");
}
